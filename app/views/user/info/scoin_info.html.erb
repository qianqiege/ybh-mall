<%= title '账户详细信息' %>
<table class="table">
    <thead>
        <tr>
            <td class="td">S货币包</td>
            <td class="td">开始时间</td>
            <td class="td">结束时间</td>
            <td class="td" style="width: 10%">收益</td>
        </tr>
    </thead>
    <% if !@scoin_record[0].nil? %>
      <tbody>
        <% @scoin_record.each do |scoin| %>
          <tr class="center">
              <td class="td" style="width: 30%"><%= scoin.scoin_type.name %></td>
              <td class="td" style="width: 30%"><%= date scoin.start_at %></td>
              <td class="td" style="width: 30%"><%= date scoin.end_at %></td>
              <td class="td" style="width: 10%">0</td>
          </tr>
        <% end %>
      </tbody>
    <% else %>
      <tbody>
          <tr class="center">
              <td class="td" style="width: 30%" >null</td>
              <td class="td" style="width: 30%" >null</td>
              <td class="td" style="width: 30%" >null</td>
              <td class="td" style="width: 10%" >0</td>
          </tr>
      </tbody>
    <% end %>
</table>
