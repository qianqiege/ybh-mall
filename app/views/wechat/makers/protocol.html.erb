<script type="text/javascript">
	$( function () {
		$("#planBtn").click( function () {
			if ($('#commit').is(":checked")) {
				$("#agree").show();
			}else {
				alert("请在承诺书前打钩")
			}
		})
		
		$("#fImg").click( function () {
			$("#agree").hide();
		})
	})
</script>