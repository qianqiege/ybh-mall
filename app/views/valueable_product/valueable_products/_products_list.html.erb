<ul class="clearfix chanpin">
	<% @valueable_products.each do |product| %>
	 	<li>
			<a href="">
				<div class="images">
					<%= image_tag(product.image_url(:product_icon), class: "value_img", alt: "产品图片") %>
				</div>
				<span><%= product.name %></span>
				<p>
					<span> <%= product.price %> </span>
		 			<span> 元 </span>
				</p>
			</a>
		</li>
	<% end%>									
</ul>